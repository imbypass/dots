{
  "activation_mode": {
    "labels": "jkl;as"
  },
  "app_launch_prefix": "",
  "as_window": false,
  "hotreload_theme": true,
  "force_keyboard_focus": true,
  "builtins": {
    "ai": {
      "anthropic": {
        "prompts": [
          {
            "label": "Artificial Intelligence",
            "max_tokens": 1000,
            "model": "claude-3-5-sonnet-20241022",
            "prompt": "You are a helpful general assistant. Keep your answers short and precise.",
            "temperature": 1
          }
        ]
      },
      "icon": "help-browser",
      "name": "ai",
      "placeholder": "AI",
      "switcher_only": true,
      "weight": 5
    },
    "applications": {
      "actions": {
        "enabled": true,
        "hide_category": false,
        "hide_without_query": true
      },
      "context_aware": true,
      "hide_actions_with_empty_query": true,
      "history": true,
      "name": "applications",
      "placeholder": " Search...",
      "icon": "",
      "prioritize_new": false,
      "refresh": true,
      "show_generic": true,
      "show_icon_when_single": true,
      "show_sub_when_single": true,
      "weight": 5,
      "theme": "base16/default"
    },
    "bookmarks": {
      "entries": [],
      "icon": "bookmark",
      "name": "bookmarks",
      "placeholder": "Bookmarks",
      "switcher_only": true,
      "weight": 5
    },
    "calc": {
      "icon": "accessories-calculator",
      "min_chars": 4,
      "name": "calc",
      "placeholder": "Calculator",
      "require_number": true,
      "weight": 5
    },
    "clipboard": {
      "avoid_line_breaks": true,
      "exec": "wl-copy",
      "image_height": 300,
      "max_entries": 3000,
      "name": "clipboard",
      "placeholder": " Search...",
      "icon": "",
      "switcher_only": true,
      "weight": 5,
      "theme": "base16/default_wide"
    },
    "commands": {
      "icon": "utilities-terminal",
      "name": "commands",
      "placeholder": "Commands",
      "switcher_only": true,
      "weight": 5
    },
    "custom_commands": {
      "icon": "utilities-terminal",
      "name": "custom_commands",
      "placeholder": "Custom Commands",
      "weight": 5
    },
    "dmenu": {
      "hidden": true,
      "name": "dmenu",
      "placeholder": " Search...",
      "icon": "",
      "switcher_only": true,
      "weight": 5
    },
    "emojis": {
      "exec": "wl-copy",
      "history": true,
      "name": "emojis",
      "theme": "base16/default_slim",
      "placeholder": " Search...",
      "icon": "",
      "show_unqualified": false,
      "switcher_only": true,
      "typeahead": true,
      "weight": 5
    },
    "finder": {
      "concurrency": 8,
      "icon": "file",
      "ignore_gitignore": true,
      "name": "finder",
      "placeholder": "Finder",
      "refresh": true,
      "show_icon_when_single": true,
      "switcher_only": true,
      "use_fd": false,
      "weight": 5
    },
    "hyprland_keybinds": {
      "path": "~/.config/hypr/core/binds.conf",
      "placeholder": " Search...",
      "icon": ""
    },
    "runner": {
      "generic_entry": false,
      "history": true,
      "icon": "utilities-terminal",
      "name": "runner",
      "placeholder": "Runner",
      "refresh": true,
      "typeahead": true,
      "weight": 5
    },
    "ssh": {
      "history": true,
      "icon": "preferences-system-network",
      "name": "ssh",
      "placeholder": "SSH",
      "refresh": true,
      "switcher_only": true,
      "weight": 5
    },
    "switcher": {
      "name": "switcher",
      "placeholder": "Switcher",
      "prefix": "/",
      "weight": 5
    },
    "symbols": {
      "after_copy": "",
      "history": true,
      "name": "symbols",
      "placeholder": "Symbols",
      "switcher_only": true,
      "typeahead": true,
      "weight": 5
    },
    "websearch": {
      "entries": [
        {
          "name": "Google",
          "url": "https://www.google.com/search?q=%TERM%"
        }
      ],
      "icon": "applications-internet",
      "name": "websearch",
      "placeholder": "Websearch",
      "weight": 5
    },
    "windows": {
      "icon": "view-restore",
      "name": "windows",
      "placeholder": "Windows",
      "show_icon_when_single": true,
      "weight": 5
    },
    "xdph_picker": {
      "hidden": true,
      "name": "xdphpicker",
      "placeholder": "Screen/Window Picker",
      "theme": "base16/default_slim",
      "show_sub_when_single": true,
      "switcher_only": true,
      "weight": 5
    }
  },
  "close_when_open": false,
  "disable_click_to_close": false,
  "events": {
    "on_activate": "",
    "on_exit": "",
    "on_launch": "",
    "on_query_change": "",
    "on_selection": ""
  },
  "keys": {
    "accept_typeahead": ["tab"],
    "activation_modifiers": {
      "alternate": "alt",
      "keep_open": "shift"
    },
    "ai": {
      "clear_session": ["ctrl x"],
      "copy_last_response": ["ctrl c"],
      "resume_session": ["ctrl r"],
      "run_last_response": ["ctrl e"]
    },
    "close": ["esc"],
    "next": ["down"],
    "prev": ["up"],
    "remove_from_history": ["shift backspace"],
    "resume_query": ["ctrl r"],
    "toggle_exact_search": ["ctrl m"],
    "trigger_labels": "lalt"
  },
  "list": {
    "dynamic_sub": true,
    "keyboard_scroll_style": "neovim",
    "max_entries": 1000,
    "placeholder": "No Results",
    "show_initial_entries": true,
    "single_click": true,
    "visibility_threshold": 20
  },
  "locale": "",
  "monitor": "",
  "plugins": [
    {
      "name": "wallpaper",
      "placeholder": " Search...",
      "icon": "",
      "theme": "base16/wallpaper",
      "switcher_only": false,
      "refresh": true,
      "src": "bash -c ~/.local/share/harmony/scripts/walker/wallpaper.sh",
      "parser": "kv"
    },
    {
      "name": "themes",
      "placeholder": " Search...",
      "icon": "",
      "theme": "base16/default_slim",
      "switcher_only": false,
      "refresh": true,
      "src": "bash -c ~/.local/share/harmony/scripts/walker/themes.sh",
      "parser": "kv"
    },
    {
      "name": "fonts",
      "placeholder": " Search...",
      "icon": "",
      "theme": "base16/default_slim",
      "switcher_only": false,
      "refresh": true,
      "src": "bash -c ~/.local/share/harmony/scripts/walker/fonts.sh",
      "parser": "kv"
    },
    {
      "name": "mainmenu",
      "search": false,
      "placeholder": "Go..",
      "icon": "",
      "theme": "base16/default_slim",
      "switcher_only": true,
      "keep_sort": true,
      "recalculate_score": true,
      "show_icon_when_single": true,
      "entries": [
        {
          "label": "    Apps",
          "exec": "walker -m applications"
        },
        {
          "label": "    Record",
          "exec": "harmony-record"
        },
        {
          "label": "    Style",
          "exec": "walker -n -m style"
        },
        {
          "label": "󰚰    Update",
          "exec": "walker -n -m update"
        },
        {
          "label": "    About",
          "exec": "foot -a Harmony -e bash -c harmony-about"
        },
        {
          "label": "⏻   System",
          "exec": "walker -H -m power"
        }
      ]
    },
    {
      "name": "style",
      "search": false,
      "placeholder": "Go..",
      "icon": "",
      "theme": "base16/default_slim",
      "switcher_only": true,
      "keep_sort": true,
      "recalculate_score": true,
      "show_icon_when_single": true,
      "entries": [
        {
          "label": "󰸉    Wallpaper",
          "exec": "omarchy-theme-bg-next"
        },
        {
          "label": "    Theme",
          "exec": "walker -n -m themes"
        },
        {
          "label": "    Font",
          "exec": "walker -n -m fonts"
        },
        {
          "label": "    Install",
          "exec": "foot -a Harmony -e bash -c harmony-theme-install"
        }
      ]
    },
    {
      "name": "update",
      "search": false,
      "placeholder": "Go..",
      "icon": "",
      "theme": "base16/default_slim",
      "switcher_only": true,
      "keep_sort": true,
      "recalculate_score": true,
      "show_icon_when_single": true,
      "entries": [
        {
          "label": "󰚰    System",
          "exec": "foot -a Harmony -e bash -c harmony-update"
        },
        {
          "label": "    Mirrors",
          "exec": "foot -a Harmony -e bash -c harmony-update-mirrors"
        }
      ]
    },
    {
      "name": "power",
      "icon": "",
      "theme": "base16/default_slim",
      "switcher_only": true,
      "keep_sort": true,
      "search": false,
      "recalculate_score": true,
      "show_icon_when_single": true,
      "entries": [
        {
          "label": "󰑓   Reload",
          "exec": "harmony-reload all"
        },
        {
          "label": "   Shutdown",
          "exec": "shutdown now"
        },
        {
          "label": "󰜉   Restart (Full)",
          "exec": "reboot"
        },
        {
          "label": "󰜉   Restart (Soft)",
          "exec": "systemctl soft-reboot"
        },
        {
          "label": "   Lock Screen",
          "exec": "hyprlock"
        }
      ]
    }
  ],
  "search": {
    "delay": 0,
    "placeholder": "Search",
    "resume_last_query": false
  },
  "terminal_title_flag": "",
  "theme": "base16/default",
  "timeout": 0,

  "icon": ""
}
